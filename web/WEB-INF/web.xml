<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>loginSRV</servlet-name>
        <servlet-class>soft.jf.seguridad.servlets.loginSRV</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>llenalistasSRV</servlet-name>
        <servlet-class>soft.jf.seguridad.servlets.llenalistasSRV</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>usuariosSRV</servlet-name>
        <servlet-class>soft.jf.seguridad.servlets.usuariosSRV</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>permisosSRV</servlet-name>
        <servlet-class>soft.jf.seguridad.servlets.permisosSRV</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>venclientesSRV</servlet-name>
        <servlet-class>soft.jf.seguridad.servlets.venclientesSRV</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>conmonedasSRV</servlet-name>
        <servlet-class>soft.jf.seguridad.servlets.conmonedasSRV</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginSRV</servlet-name>
        <url-pattern>/loginSRV</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>llenalistasSRV</servlet-name>
        <url-pattern>/llenalistasSRV</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>usuariosSRV</servlet-name>
        <url-pattern>/usuariosSRV</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>permisosSRV</servlet-name>
        <url-pattern>/permisosSRV</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>venclientesSRV</servlet-name>
        <url-pattern>/venclientesSRV</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>conmonedasSRV</servlet-name>
        <url-pattern>/conmonedasSRV</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
